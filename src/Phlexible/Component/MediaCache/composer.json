{
    "name": "phlexible/media-cache",
    "description": "phlexible media cache component",
    "license": "MIT",
    "type": "library",
    "keywords": ["phlexible", "media", "cache"],
    "autoload": {
        "psr-4": {
            "Phlexible\\Component\\MediaCache\\": ""
        }
    },
    "require": {
        "php": ">=5.6.0",
        "phlexible/media-extractor": "~1.4",
        "phlexible/media-manager": "~1.4",
        "phlexible/media-template": "~1.4",
        "phlexible/media-type": "~1.4",
        "psr/log": "~1.0",
        "symfony/config": "~2.8"
    },
    "require-dev": {
        "doctrine/orm": "~2.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.4-dev"
        }
    },
    "repositories": [{
        "type": "path",
        "url": "../MediaExtractor"
    },{
        "type": "path",
        "url": "../MediaTemplate"
    },{
        "type": "path",
        "url": "../MediaType"
    },{
        "type": "path",
        "url": "../MediaManager"
    }],
    "minimum-stability": "dev",
    "prefer-stable": true
}
