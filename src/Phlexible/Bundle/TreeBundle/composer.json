{
    "name": "phlexible/tree-bundle",
    "description": "phlexible tree bundle",
    "license": "proprietary",
    "type": "symfony-bundle",
    "keywords": ["phlexible"],
    "autoload": {
        "psr-4": {
            "Phlexible\\Bundle\\TreeBundle\\": ""
